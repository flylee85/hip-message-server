<PRPA_IN201315UV02 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ITSVersion="XML_1.0" xsi:schemaLocation="urn:hl7-org:v3 ../multicacheschemas/PRPA_IN201316UV02.xsd" xmlns="urn:hl7-org:v3">
    <id root="2.16.156.10011.2.5.1.1" th:attr="extension=${returnMsgId}"/>
    <creationTime th:attr="value=${#dates.format(#dates.createNow(), 'yyyyMMddHHmmss')}"/>
    <interactionId root="2.16.156.10011.2.5.1.2" extension="PRPA_IN201315UV02"/>
    <processingCode code="P"/>
    <processingModeCode code="R"/>
    <acceptAckCode code="NE"/>
    <receiver typeCode="RCV">
        <device classCode="DEV" determinerCode="INSTANCE">
            <id root="2.16.156.10011.2.5.1.3" th:attr="extension=${sendId}"/>
        </device>
    </receiver>
    <sender typeCode="SND">
        <device classCode="DEV" determinerCode="INSTANCE">
            <id root="2.16.156.10011.2.5.1.3" th:attr="extension=${receiveId}"/>
        </device>
    </sender>
    <acknowledgement th:attr="typeCode=${returnStatusCode}">
        <!--请求消息ID-->
        <targetMessage>
            <id root="2.16.156.10011.2.5.1.1" th:attr="extension=${msgId}"/>
        </targetMessage>
        <acknowledgementDetail>
            <text th:text="${returnStatusExplain}"/>
        </acknowledgementDetail>
    </acknowledgement>
    <controlActProcess classCode="CACT" moodCode="EVN">
		<subject typeCode="SUBJ">
			<registrationEvent classCode="REG" moodCode="EVN">
				<statusCode code="active"/>
				<subject1 typeCode="SBJ">
					<patient classCode="PAT">
						<id root="2.16.156.10011.0.2.1" th:attr="extension=${patientId}"/>
						<statusCode code="active"/>
						<patientPerson classCode="PSN" determinerCode="INSTANCE">
							<name use="L" th:text="${name}" />
						</patientPerson>
					</patient>
				</subject1>
				<custodian typeCode="CST">
					<assignedEntity classCode="ASSIGNED">
						<id root="2.16.156.10011.0.3.2" th:attr="extension=${authorCode}"/>
						<assignedPerson classCode="PSN" determinerCode="INSTANCE">
							<name use="L" th:text="${authorName}"/>
						</assignedPerson>
					</assignedEntity>
				</custodian>
			</registrationEvent>
		</subject>
	</controlActProcess>
</PRPA_IN201315UV02>
